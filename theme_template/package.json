{
  "name": "sharesolutions",
  "version": "1.0.0",
  "main": "gulpfile.js",
  "scripts": {
    "----tools----": "echo \"development\" && exit 0",
    "_kill_orphan_php_server": "kill -9 $(lsof -t -i:8887)",
    "_preinstall__run_if_needed__with_sudo_if_needed": "npm rm -g gulp && npm install -g gulp-cli",
    "----development----": "echo \"development\" && exit 0",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "gulp --gulpfile src/gulp/gulpfile.js --cwd ./ cleanup",
    "fixes": "npm run fix-css-lint && npm run fix-js",
    "fix-css-lint": "echo \"=========== CSS-LINT ===========\"; stylefmt -r \"src/resources/styles/**/*.scss\" && stylelint \"src/resources/styles/**/*.scss\" --syntax scss; exit 0",
    "fix-js": "echo \"=========== JS-FIX ===========\"; standard --fix --verbose | snazzy; exit 0",
    "php-migration-up": "php ./src/start/migrations.php up",
    "php-migration-down": "php ./src/start/migrations.php down",
    "develop": "gulp --gulpfile src/gulp/gulpfile.js --cwd ./",
    "php-unit": "./src/vendor/bin/phpunit --bootstrap ../../../wp-config.php --testdox src/tests/",
    "----deployment----": "echo \"development\" && exit 0",
    "frontend-bundle": "echo \"=========== BUNDLE ===========\"; gulp --gulpfile src/gulp/gulpfile.js --cwd ./ beforeDeploy",
    "frontend-deploy": "npm run fixes && NODE_ENV=production npm run frontend-bundle",
    "tag-last-commit-as-deploy": "gulp --gulpfile src/gulp/gulpfile.js --cwd ./ add-deploy-tag-to-last-commit",
    "pack": "echo \"=========== PACK ===========\"; gulp --gulpfile src/gulp/gulpfile.js --cwd ./ pack",
    "deploy": "npm run frontend-deploy; npm run pack"
  },
  "author": "Share Solutions",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "array-each": "^1.0.1",
    "assignment": "^2.2.1",
    "babel-eslint": "^7.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babelify": "^7.3.0",
    "bootstrap-sass": "^3.3.7",
    "browser-sync": "^2.23.6",
    "browserify": "^14.1.0",
    "classnames": "^2.2.5",
    "core-decorators": "^0.17.0",
    "del": "^3.0.0",
    "ftp": "^0.3.10",
    "glob": "^7.1.1",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-babel": "^7.0.1",
    "gulp-clean-css": "^3.4.2",
    "gulp-concat": "^2.6.1",
    "gulp-connect-php": "^1.0.1",
    "gulp-each": "^0.5.0",
    "gulp-if": "^2.0.2",
    "gulp-imagemin": "^4.1.0",
    "gulp-include": "^2.3.1",
    "gulp-notify": "^3.0.0",
    "gulp-rev": "^7.1.2",
    "gulp-sass": "^3.1.0",
    "gulp-sass-bulk-import": "^1.0.1",
    "gulp-sourcemaps": "^2.6.0",
    "gulp-tap": "^0.1.3",
    "gulp-uglify": "^3.0.0",
    "gulp-watch": "^4.3.11",
    "gulp.spritesmith": "^6.5.0",
    "imagemin-mozjpeg": "^7.0.0",
    "imagemin-pngquant": "^5.0.1",
    "in-array": "^0.1.2",
    "intl": "^1.2.5",
    "intl-locales-supported": "^1.0.0",
    "jest-cli": "^19.0.2",
    "jquery": "^3.2.1",
    "moment": "^2.18.1",
    "promise-polyfill": "^6.0.2",
    "qs": "^6.4.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "redux": "^3.6.0",
    "redux-devtools": "^3.3.2",
    "redux-logger": "^3.0.0",
    "redux-thunk": "^2.2.0",
    "require-dir": "0.3.2",
    "require-globify": "^1.4.1",
    "rev-del": "^1.0.5",
    "snazzy": "^6.0.0",
    "standard": "^10.0.0",
    "stylefmt": "^5.3.2",
    "stylelint": "^7.10.1",
    "stylelint-config-standard": "^16.0.0",
    "vinyl-buffer": "^1.0.0",
    "yazl": "^2.4.3"
  },
  "description": "",
  "standard": {
    "ignore": [
      "/build/**",
      "gulpfile.js",
      "/src/gulp/tasks/**"
    ],
    "globals": [
      "$",
      "jQuery",
      "jest",
      "describe",
      "it",
      "expect",
      "FormData",
      "localStorage"
    ],
    "parser": "babel-eslint"
  }
}
